<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Dark">
			<!-- Color Overrides -->
			<Color x:Key="ApplicationPageBackgroundTheme">#222222</Color>
			<SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="{StaticResource ApplicationPageBackgroundTheme}" />

			<!-- Margin/Padding Overrides -->
			<Thickness x:Key="MenuBarItemButtonPadding">10,4</Thickness>
			<Thickness x:Key="MenuFlyoutItemThemePaddingNarrow">12,4</Thickness>
			<Thickness x:Key="TextControlThemePadding">10,4,4,4</Thickness>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>